{
  "name": "vue3-juegos-api",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "JAVI-CC JUEGOS API",
  "author": "JAVI-CC",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest",
    "test:unit:ci": "cypress run-ct",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "cypress:open": "concurrently \"quasar dev\" \"cypress open\""
  },
  "dependencies": {
    "@capacitor/cli": "^3.3.3",
    "@capacitor/core": "^3.3.3",
    "@quasar/cli": "^1.3.1",
    "@quasar/extras": "^1.13.3",
    "@quasar/icongenie": "^2.4.2",
    "@vuelidate/core": "^2.0.0-alpha.32",
    "@vuelidate/validators": "^2.0.0-alpha.25",
    "axios": "^0.21.1",
    "concurrently": "^7.0.0",
    "core-js": "^3.6.5",
    "cypress": "9.3.1",
    "laravel-echo": "^1.11.3",
    "pusher-js": "^7.0.3",
    "quasar": "^2.0.0",
    "vue-router-mock": "^0.1.3",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^3.0.0",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@quasar/quasar-app-extension-testing": "^2.0.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.0.0-beta.9",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.7",
    "electron": "^16.0.7",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.4.0",
    "workbox-webpack-plugin": "^6.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
